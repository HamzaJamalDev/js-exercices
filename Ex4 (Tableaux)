<body>
    
    <script>
        
        function rechercheBinaire(tab, obj) {
            let debut = 0;
            let fin = tab.length - 1;
        
            while (debut <= fin) {
                let milieu = debut + ((fin - debut) >> 1);
                if (tab[milieu] === obj) {
                    return milieu;
                } else if (tab[milieu] < obj) {
                    debut = milieu + 1;
                } else {
                    fin = milieu - 1;
                }
            }
            return -1;
        }

    </script>
    
</body>
</html>
